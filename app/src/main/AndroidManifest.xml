<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.thiago.findjob" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/CustomTheme"
        android:name="com.example.thiago.findjob.extras.AppController">
        <activity
            android:name=".activitys.PrincipalAluno"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name=".activitys.SplashScreen"
            android:label="@string/title_activity_splash_screen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activitys.Login"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".activitys.Cadastrar"
            android:label="@string/title_activity_cadastrar" >
        </activity>
        <activity
            android:name=".activitys.PrincipalEmpresa"
            android:label="@string/title_activity_principal_empresa" >
        </activity>

    </application>

</manifest>
